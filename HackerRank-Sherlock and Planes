#include <bits/stdc++.h>

using namespace std;



int main()
{
   int t,x1,y1,z1,x2,y2,z2,x3,y3,z3,x4,y4,z4;
   cin>>t;
   while(t--)
   {
       cin>>x1>>y1>>z1;
       cin>>x2>>y2>>z2;
       cin>>x3>>y3>>z3;
       cin>>x4>>y4>>z4;

       if(x1==x2&&x2==x3&&x3==x4)
       {
           cout<<"YES"<<endl;
           continue;
       }
       if(y1==y2&&y2==y3&&y3==y4)
       {
           cout<<"YES"<<endl;
           continue;
       }
       if(z1==z2&&z2==z3&&z3==z4)
       {
           cout<<"YES"<<endl;
           continue;
       }
       cout<<"NO"<<endl;
   }

    return 0;
}
