#include<bits/stdc++.h>
using namespace std;

int main()

{
    int t,a;
    cin>>t;
    vector< pair<int,int> >v;
    for(int i=0;i<t;i++)
    {
        cin>>a;
        v.push_back(make_pair(a,i));
    }
    sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i].second<<" ";
    }
    cout<<endl;
    return 0;
}
